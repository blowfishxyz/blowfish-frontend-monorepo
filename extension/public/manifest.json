{
  "manifest_version": 3,
  "name": "Blowfish Wallet Protect BETA",
  "description": "Blowfish API protection for injected web3 wallets",
  "version": "0.0.4",
  "options_ui": {
    "page": "popup.html"
  },
  "action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "js/vendor.js",
        "js/content-scripts/inject-scripts.js",
        "js/content-scripts/window-messages.js"
      ],
      "css": ["styles.css"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["js/injected/proxy-window-evm.js", "js/vendor.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [],
  "host_permissions": ["<all_urls>"]
}

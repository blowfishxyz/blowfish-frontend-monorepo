{
  "name": "@blowfish/ui",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Package containing reusable UI components",
  "private": true,
  "scripts": {
    "build": "rm -rf build && tsup",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "@blowfish/hooks": "*",
    "@blowfish/utils": "*",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "@blowfish/tsconfig": "*",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "^5.1.26",
    "tsup": "^6.6.3",
    "typescript": "4.9.5"
  },
  "exports": {
    "./icons": {
      "types": "./build/icons.d.ts",
      "import": "./build/icons.mjs",
      "require": "./build/icons.js"
    },
    "./core": {
      "types": "./build/core.d.ts",
      "import": "./build/core.mjs",
      "require": "./build/core.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  }
}
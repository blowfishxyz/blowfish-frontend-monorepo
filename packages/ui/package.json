{
  "name": "@blowfishxyz/ui",
  "license": "MIT",
  "version": "0.0.1-beta.8",
  "description": "Package containing Blowfish reusable UI components",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "README.md",
    "src"
  ],
  "scripts": {
    "build": "rm -rf build && tsup",
    "build:dev": "rm -rf build && tsup --watch",
    "build:storybook": "TS_NODE_PROJECT=.storybook/tsconfig.json build-storybook",
    "prepare": "pnpm run build",
    "typecheck": "tsc --noEmit",
    "storybook": "TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 6006"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.0",
    "@floating-ui/react": "^0.23.1",
    "decimal.js": "^10.4.3",
    "deepmerge": "^4.3.1",
    "react-query": "^3.39.3",
    "styled-components": "6.0.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@blowfish/tsconfig": "workspace:*",
    "@blowfishxyz/api-client": "workspace:*",
    "@blowfish/utils": "workspace:*",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-styling": "^0.3.1",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "18.0.11",
    "csstype": "^3.1.2",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "6.5.16",
    "styled-components": "6.0.4",
    "tsup": "^6.6.3",
    "typescript": "4.9.5"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    }
  }
}

  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf build && tsup"
  },
  "devDependencies": {
    "@blowfish/tsconfig": "*",
    "tsup": "^6.6.3",
    "typescript": "4.9.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "8.35.0"
